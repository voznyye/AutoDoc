{
  "name": "ai-documentation-generator",
  "displayName": "AI Documentation Generator",
  "description": "AI-powered VS Code/Cursor extension that automatically generates and maintains comprehensive project documentation using DeepSeek R1T2 Chimera AI model",
  "version": "2.6.0",
  "publisher": "voznyye",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "ai-documentation",
    "auto-documentation",
    "ai-powered",
    "documentation-generator",
    "project-documentation",
    "readme-generator",
    "code-analysis",
    "deepseek",
    "free-ai",
    "git-integration",
    "markdown",
    "typescript",
    "javascript",
    "python",
    "intelligent-docs"
  ],
  "activationEvents": [
    "workspaceContains:**/.git"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-doc-generator.generate",
        "title": "Generate Project Documentation",
        "category": "AI Doc Generator",
        "icon": "$(book)"
      },
      {
        "command": "ai-doc-generator.configure",
        "title": "Configure AI Settings",
        "category": "AI Doc Generator",
        "icon": "$(settings-gear)"
      },
      {
        "command": "ai-doc-generator.toggleAutoUpdate",
        "title": "Toggle Auto-Update Documentation on Commit",
        "category": "AI Doc Generator",
        "icon": "$(sync)"
      }
    ],
    "configuration": {
      "title": "AI Documentation Generator",
      "properties": {
        "ai-doc-generator.apiToken": {
          "type": "string",
          "description": "DeepSeek R1T2 Chimera API Token",
          "scope": "application"
        },
        "ai-doc-generator.autoUpdateOnCommit": {
          "type": "boolean",
          "default": false,
          "description": "Auto-update documentation after git commits (optional)"
        },
        "ai-doc-generator.excludePatterns": {
          "type": "array",
          "default": [
            "node_modules/**",
            "dist/**",
            "build/**",
            "*.test.*",
            "*.spec.*",
            "**/*.d.ts"
          ],
          "description": "File patterns to exclude from documentation analysis"
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "aiDocGeneratorView",
          "name": "AI Documentation",
          "when": "true"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "ai-doc-generator.generate",
          "when": "workspaceFolderCount != 0"
        },
        {
          "command": "ai-doc-generator.configure"
        },
        {
          "command": "ai-doc-generator.toggleAutoUpdate"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "package": "vsce package",
    "test": "npm run compile && node ./out/test/runTest.js",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "release:patch": "npm version patch && npm run compile && npm run package && git add . && git commit -m \"Release v$(node -p \"require('./package.json').version\")\" && git tag v$(node -p \"require('./package.json').version\")",
    "release:minor": "npm version minor && npm run compile && npm run package && git add . && git commit -m \"Release v$(node -p \"require('./package.json').version\")\" && git tag v$(node -p \"require('./package.json').version\")",
    "release:major": "npm version major && npm run compile && npm run package && git add . && git commit -m \"Release v$(node -p \"require('./package.json').version\")\" && git tag v$(node -p \"require('./package.json').version\")",
    "release:custom": "npm run compile && npm run package",
    "publish": "vsce publish",
    "publish:patch": "npm run release:patch && vsce publish",
    "publish:minor": "npm run release:minor && vsce publish",
    "publish:major": "npm run release:major && vsce publish",
    "publish:custom": "npm run release:custom && vsce publish",
    "login": "vsce login voznyye",
    "logout": "vsce logout voznyye",
    "verify": "vsce verify-pat voznyye",
    "clean": "rm -rf out dist *.vsix",
    "build:clean": "npm run clean && npm run compile",
    "package:clean": "npm run clean && npm run compile && npm run package"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^6.0.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^17.2.1",
    "markdown-it": "^13.0.0",
    "simple-git": "^3.19.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voznyye/AutoDoc"
  },
  "bugs": {
    "url": "https://github.com/voznyye/AutoDoc/issues"
  },
  "homepage": "https://github.com/voznyye/AutoDoc#readme",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "icon": "media/icon.png",
  "license": "MIT"
}
